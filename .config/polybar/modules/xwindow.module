;[module/xwindow]
; shows the title of the currently focused window
type = internal/xwindow

; Available tags:
;   <label> (default)
;format-background = #f00
;format-foreground = #000
format-padding = 4

; Available tokens:
;   %title%
; Default: %title%
label = %title:0:120:...%
label-maxlen = 50
label-padding = 4
