    comboMode:
      activationThreshold: 5
    particles:
      colours:
        type: "random"
    playAudio:
      enabled: true
    screenShake:
      maxIntensity: 5
  "atom-beautify":
    general:
      _analyticsUserId: "365efe89-a9f8-4048-9982-85cc252cd6c5"
  "autocomplete-clang":
    includeNonDoxygenCommentsAsDocumentation: true
  core:
