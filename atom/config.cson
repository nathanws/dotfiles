      "metrics"
      "exception-reporting"
      "language-clojure"
      "language-csharp"
      "language-coffee-script"
      "language-perl"
      "language-php"
      "language-python"
      "language-ruby"
      "language-ruby-on-rails"
      "language-toml"
    ]
    telemetryConsent: "no"
  editor: {}
  "exception-reporting":
    userId: "ad477324-f7fe-a0fb-79ad-e73a32ad5c07"
  "markdown-preview":
    liveUpdate: false
    useGitHubStyle: true
  "tree-ignore": {}
  "tree-view":
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
