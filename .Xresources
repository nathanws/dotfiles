URxvt.intensityStyles:	    false
URxvt.background:           [70]#000000
URxvt.depth:                32
URxvt.scrollBar:            false
URxvt.cursorColor:          white
URxvt.perl-ext-common:      default,matcher,resize-font,font-size
URxvt.url-launcher:         /usr/bin/xdg-open
URxvt.matcher.button:       1
URxvt.resize-font.smaller:  C-Down
URxvt.resize-font.bigger:   C-Up
URxvt.colorUL:              #4682B4
URxvt.font:                 xft:hack:size=9

!!rofi.color-enabled:         true
!!rofi.color-window:          argb:cf2d112a, argb:02d112a, argb:02d112a
!!rofi.color-normal:          argb:02d112a, #fdf6e3, argb:02d112a, argb:02d112a, #fc5f39
!!rofi.color-active:          argb:02d112a, argb:79fdf6e3, argb:02d112a, argb:02d112a, argb:82fc5f39
!!rofi.color-urgent:          argb:02d112a, #f2343f, argb:02d112a, argb:02d112a, #f2343f
!rofi.fake-transparency:	    true
!rofi.fake-background:       'screenshot'
!rofi.lines:		            3
!rofi.bw:		            0
!rofi.opacity:		        "10"
!rofi.hide-scrollbar:	    true
!rofi.width:                 30
